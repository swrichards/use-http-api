        "cross-env": "^5.2.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-prettier": "^3.1.0",
        "gh-pages": "^2.0.1",
        "react": "~16.8",
        "react-dom": "^16.8.6",
        "react-scripts": "^3.0.1",
        "rollup": "^1.1.2",
        "rollup-plugin-babel": "^4.3.2",
